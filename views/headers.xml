                    <img src="/sitio_imagen/static/src/img/lupa-blue.png" alt=""/>
                </a>
                <a class="a-cart-top cart-other-page" href="#">
                    <img src="/sitio_imagen/static/src/img/cart-blue.png" alt=""/>
                </a>
            </div>
        </div>

    </template>

    <template id="cart_slide">
        <div class="detail-header">
            <h5 class="h-title-cart">Mi carrito</h5>
        </div>
        <div class="detail-body">
            <div t-if="not website_sale_order or not website_sale_order.website_order_line" class="alert alert-info">
                <p class="text-center text-empty-cart">Tu carrito está vacio.</p>
                <p class="text-center text-animation-cart">Atrévete a comprar nuestros productos.</p>
            </div>
            <t t-if="website_sale_order and website_sale_order.website_order_line">
                <t t-foreach="lines_product" t-as="line">
                    <div class="item-product">
                        <div class="item-img">
                            <span t-field="line.product_id.image_128"
                                  t-options="{'widget': 'image', 'qweb_img_responsive': False, 'class': 'rounded o_image_64_max mb-2'}"/>
                        </div>
                        <div class="item-description">
                            <div class="row">
                                <div class="col-md-12">
                                    <a class="a-product-name" href="#description-product" data-toggle="collapse"
                                       aria-expanded="false" aria-controls="description-product">
                                        <span t-esc="line.name_short"/>
                                    </a>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <span class="sr-only">Four out of Five Stars</span>
                                    <i class="fas fa-star bg-start-span start-i"></i>
                                    <i class="fas fa-star bg-start-span start-i"></i>
                                    <i class="fas fa-star bg-start-span start-i"></i>
                                    <i class="fas fa-star bg-start-span start-i"></i>
                                    <i class="far fa-star bg-start-span start-i"></i>
                                    <span class="label label-success label-start-number">(61)</span>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <h5 class="product-price">
                                        <span t-esc="line.price_total"
                                              t-options="{'widget': 'monetary', 'display_currency': website.currency_id}"
                                        />
                                    </h5>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <p id="description-product" class="description-product text-justify collapse"
                                       t-esc="line.product_template_id.intro_text">
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                </t>
                <div class="text-center mb-3">
                    <span class="h6">
                        <t t-call="website_sale.total">
                            <t t-set='hide_coupon'>True</t>
                        </t>
                    </span>

                    <div class="row">
                        <div class="col-12 col-md-12 col-xl-12">
                            <p class="delivery-message">
                                *Para que puedas recibir tu producto con nuestro servicio de envío, tu consumo debe ser
                                de $15 dólares en adelante. El
                                costo es adicional
                            </p>
                        </div>
                    </div>
                    <a role="button" class="btn btn-primary bg-blue-website border-radius125" href="/shop/cart">
                        Ver carrito (
                        <t t-esc="website_sale_order.cart_quantity"/>
                        productos)
                    </a>
                </div>
            </t>
        </div>
    </template>

    <template id="header_portal">
        <div class="row bg-second pl-1">
            <div class="col-12 col-md-2 col-xl-2">
                <div class="row">
                    <div class="col-md-4 col-xl-2 text-center">
                        <a class="a-img-linea-nav-hidden ml-xl-2 ml-mb-2" href="#">
                            <img src="/sitio_imagen/static/src/img/linea-blue.png" alt="Linea"/>
                        </a>
                    </div>
                    <div class="col-md-8 col-xl-10">
                        <div class="row">
                            <div class="col-md-12 col-xl-12">
                                <a class="a-img-logo-nav-hidden ml-xl-2 mt-xl-1 mt-md-1" href="/shop">
                                    <img class="img-logo ml-xl-2" src="/sitio_imagen/static/src/img/logo.png"
                                         alt="Logo de la empresa"/>
                                </a>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12 col-xl-12 mb-xl-1 ml-xl-2 pl-xl-4">
                                <p class="text-slogan-small ml-xl-2">Libros Recreativos</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-12 col-md-10 col-xl-10">
                <div class="row">
                    <div class="col-12 col-md-12 col-xl-12 text-rigth mt-xl-2 mt-md-3">
                        <nav class="navbar navbar-expand-md">
                            <div class="container">
                                <div class="collapse navbar-collapse" id="top_menu_collapse">
                                    <!-- FIXME We want menu to open to the right by default (except cases handled in JS) -->
                                    <ul class="nav navbar-nav ml-auto text-right" id="top_menu">
                                        <li class="nav-item divider"/>
                                        <li class="nav-item">
                                            <a href="/shop" class="nav-link link-text-portal">
                                                <b>
                                                    Tienda
                                                </b>
                                            </a>
                                        </li>
                                        <li class="nav-item divider"/>
                                        <li class="nav-item">
                                            <a href="/contact" class="nav-link link-text-portal">
                                                <b>
                                                    Contáctanos
                                                </b>
                                            </a>
                                        </li>
                                        <li class="nav-item divider" t-ignore="true" t-if="not user_id._is_public()"/>
                                        <li class="nav-item dropdown" t-ignore="true" t-if="not user_id._is_public()">
                                            <a href="#" class="nav-link dropdown-toggle link-text-portal"
                                               data-toggle="dropdown">
                                                <b>
                                                    <span t-esc="user_id.name[:23] + '...' if user_id.name and len(user_id.name) &gt; 25 else user_id.name"/>
                                                </b>
                                            </a>
                                            <div class="dropdown-menu js_usermenu link-text-portal" role="menu">
                                                <a id="o_logout" class="dropdown-item"
                                                   t-attf-href="/web/session/logout?redirect=/" role="menuitem">Cerrar
                                                    sesión
                                                </a>
                                            </div>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </nav>
                    </div>
                </div>
            </div>
        </div>

    </template>

    <!--Encabezado para el sitio de imagen-->
    <template id="header_imagen" name="Header Imagen">
        <t t-set="urls" t-value="['/shop/product','/shop/cart','/shop/address','/shop/checkout','/shop/payment','/shop/confirmation','/shop/confirm_order','/shop/extra_info','/shop/terms','/shop/thanks', '/shop/contact', '/payment/process', '/shop/kit-ludico-matematico', '/shop/kit-email', '/shop/plantilla/', '/shop/curso/registrate']"/>
<!--        <t t-set="urls_cat" t-value="['http://localhost:8050/shop', 'http://localhost:8050/shop/libros-chicos-2-4', 'http://localhost:8050/shop/libros-chicos-4-5', 'http://localhost:8050/shop/libros-chicos-6-7', 'http://localhost:8050/shop/libros-chicos-8-9', 'http://localhost:8050/shop/libros-chicos-10-12']"/>-->
        <t t-set="urls_cat" t-value="['http://chicosimagen.com/shop', 'http://chicosimagen.com/shop/libros-chicos-2-4', 'http://chicosimagen.com/shop/libros-chicos-4-5', 'http://chicosimagen.com/shop/libros-chicos-6-7', 'http://chicosimagen.com/shop/libros-chicos-8-9', 'http://chicosimagen.com/shop/libros-chicos-10-12']"/>
        <t t-set="urls_portal" t-value="['/my/orders', '/my/home', '/my/account']"/>

        <t t-set="es_portal" t-value="0"/>
        <t t-foreach="urls_portal" t-as="u">
            <t t-if="u in request.httprequest.url">
                <t t-set="es_portal" t-value="1"/>
            </t>
        </t>

        <nav class="nav-imagen">
            <t t-call="sitio_imagen.mobil_header"/>

            <t t-set="valido" t-value="0"/>
            <t t-foreach="urls" t-as="u">
                <t t-if="u in request.httprequest.url">
                    <t t-set="valido" t-value="1"/>
                </t>
            </t>

            <t t-set="valido_cat" t-value="0"/>
            <t t-foreach="urls_cat" t-as="u">
                <t t-if="u == request.httprequest.url">
                    <t t-set="valido_cat" t-value="1"/>
                </t>
            </t>

            <!--http://chicosimagen.com/shop-->
            <t t-if="valido_cat == 1">
                <div t-if="id_svg" t-att-id="id_svg"></div>
                <div class="nav-browser">
                    <t t-call="sitio_imagen.shop_header"/>
                </div>
            </t>
            <t t-elif="valido == 1">
                <div class="nav-browser">
                    <t t-call="sitio_imagen.other_header"/>
                </div>
            </t>
            <t t-else="">
                <!--<p t-esc="request.httprequest.url"></p>-->
            </t>

            <t t-if="es_portal == 1">
                <div class="nav-browser">
                    <t t-call="sitio_imagen.header_portal"/>
                </div>
                <style type="text/css">
                    main {
                    margin-top: 80px !important;
                    }
                </style>
            </t>
        </nav>

        <!--Nab book-->
        <t t-call="sitio_imagen.nav_book_page"/>
        <!--Nab cart-->
        <t t-call="sitio_imagen.nav_cart_page"/>
    </template>
</odoo>